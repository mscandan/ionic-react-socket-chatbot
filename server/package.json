{
  "name": "server",
  "version": "1.0.0",
  "description": "MindBehind Case Study, Server App",
  "main": "index.ts",
  "author": "Mehmet Selcuk Candan",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev src/app.ts",
    "eslint": "eslint ./src --ext .jsx,.ts,.tsx,.js --cache",
    "prettier": "prettier \"src/**/*\"",
    "eslint:fix": "yarn eslint --fix",
    "prettier:fix": "yarn prettier --check",
    "lint": "yarn eslint && yarn prettier --list-different",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "type-check": "yarn tsc --noEmit",
    "check-rule": "yarn type-check && yarn lint"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
